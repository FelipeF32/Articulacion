{
  "name": "ecommerce-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de E-commers con Node.js, Express y Squelize",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
